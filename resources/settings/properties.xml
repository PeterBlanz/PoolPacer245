<resources>
    <properties>
    	<property id="targetPace" type="float">120</property>
    	<property id="vibrationCount" type="number">4</property>
    	<property id="vibrationPeriod" type="number">250</property>
    </properties>
 
    <settings>
        <setting propertyKey="@Properties.targetPace" title="Target pace for 100 m (s)">
            <settingConfig type="numeric" min="1" max="600" />
		</setting>
		<setting propertyKey="@Properties.vibrationCount" title="Number of vibration periods">
            <settingConfig type="numeric" min="1" max="20" />
		</setting>
		<setting propertyKey="@Properties.vibrationPeriod" title="Vibration period (ms)">
            <settingConfig type="numeric" min="1" max="10000" />
		</setting>
    </settings>
</resources>